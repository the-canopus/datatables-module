{
    "name": "the-canopus/datatables-module",
    "type": "laravel-module",
    "license": "MIT",
    "description": "Canopus datatable module",
    "authors": [
        {
            "name": "Soheil Rahmat",
            "email": "soheilrt314@gmail.com"
        }
    ],
    "require": {
        "joshbrw/laravel-module-installer": "0.*",
        "laravel/framework": "^6.0",
        "yajra/laravel-datatables": "1.5.*",
        "yajra/laravel-datatables-fractal": "1.5.*",
        "nwidart/laravel-modules": "6.2.*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "suggest": {
        "yajra/laravel-datatables-buttons": "4.8.*",
        "yajra/laravel-datatables-editor": "1.17.*",
        "yajra/laravel-datatables-html": "4.20.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Datatables\\Providers\\DatatablesServiceProvider"
            ],
            "aliases": {
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Datatables\\": ""
        }
    }
}
